 @prefix myentity:    <http://www.myentity.example.com/> . 
 @prefix provone: <http://dataone.org/ns/provone#>  . 

myentity:EmphasizedHelloWorld a provone:Workflow  ;
	provone:used "provided_greeting"  ;
	provone:used "provided_emphasis" ;
	provone:used "emphasis_count" ;
	provone:generate "displayed_greeting" 
myentity:emphasize_greeting a provone:Workflow  ;
	provone:used "provided_greeting"  ;
	provone:used "provided_emphasis"  ;
	provone:wasInformedBy myentity:emphasize_greeting ;
	provone:wasInformedBy myentity:print_greeting .

myentity:emphasize_greeting a provone:ProcessExec  ;
	provone:used "provided_greeting"  ;
	provone:used "provided_emphasis" ;
	provone:used "emphasis_count" ;
	provone:generate "emphasized_greeting" .

myentity:print_greeting a provone:ProcessExec  ;
	provone:used "emphasized_greeting" ;
	provone:generate "displayed_greeting" .

